{
  "name": "generator-fountain-vue",
  "version": "1.0.0",
  "description": "Yeoman Fountain generator to scaffold a webapp with Vue.js written in ES6 (Babel) through Webpack including tools Gulp 4, ESLint, Browsersync and Karma",
  "homepage": "",
  "repository": "fountainjs/generator-fountain-vue",
  "author": "The Fountain Team",
  "contributors": [
    "Matthieu Lux <matthieu.lux@gmail.com> (http://swiip.github.io/)",
    "Mehdy Dara <mdara@eleven-labs.com> (http://eleven-labs.com/)",
    "Micael Mbagira <micael.mbagira@icloud.com> (http://micaelmbagira.github.io)"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "gulp",
    "gulpfile",
    "fountain",
    "cli",
    "vue"
  ],
  "dependencies": {
    "fountain-generator": "^1.0.0",
    "generator-fountain-gulp": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "cross-spawn": "^5.0.1",
    "eslint": "^3.15.0",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.1.1",
    "gulp-nsp": "^2.4.2",
    "nyc": "^10.1.2"
  },
  "nyc": {
    "include": [
      "generators/**/*.js"
    ],
    "exclude": [
      "generators/**/templates/**"
    ]
  },
  "scripts": {
    "test": "gulp",
    "prepublish": "gulp prepublish"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "xo-space/esnext",
    "rules": {
      "func-names": ["error", "as-needed"],
      "quote-props": ["error", "consistent"]
    }
  }
}
